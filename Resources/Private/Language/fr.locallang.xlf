		<body>
			<trans-unit id="tx_vibeo_domain_model_media" resname="tx_vibeo_domain_model_media"  approved="yes">
				<source>Media element (Video or audio file)</source>
				<target>Élément média (Fichier vidéo ou audio)</target>
			</trans-unit>
			<trans-unit id="tx_vibeo_domain_model_media.title" resname="tx_vibeo_domain_model_media.title"  approved="yes">
				<source>Title of this element</source>
				<target>Titre de cet élément</target>
			</trans-unit>
			<trans-unit id="tx_vibeo_domain_model_media.subtitle" resname="tx_vibeo_domain_model_media.subtitle"  approved="yes">
				<source>Subtitle of this element</source>
				<target>Sous-titre de cet élément</target>
			</trans-unit>
			<trans-unit id="tx_vibeo_domain_model_media.author" resname="tx_vibeo_domain_model_media.author"  approved="yes">
				<source>Author of this video or audio content</source>
				<target>Auteur de cet élément</target>
			</trans-unit>
			<trans-unit id="tx_vibeo_domain_model_media.description" resname="tx_vibeo_domain_model_media.description"  approved="yes">
				<source>Text description for this element</source>
