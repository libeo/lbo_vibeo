			</trans-unit>
			<trans-unit id="player.rewindText" resname="player.rewindText"  approved="yes">
				<source>Rewind</source>
				<target>Reculer</target>
			</trans-unit>
			<trans-unit id="player.forwardText" resname="player.forwardText"  approved="yes">
				<source>Forward</source>
